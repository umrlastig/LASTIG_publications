@article{ WOS:000969944800001,
Author = {Galopin, Sylvain and Touya, Guillaume and Aupinel, Pierrick and Richard,
   Freddie-Jeanne},
Title = {GeoDanceHive: An Operational Hive for Honeybees Dances Recording},
Journal = {ANIMALS},
