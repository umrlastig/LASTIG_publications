@article{ WOS:000592840100001,
Author = {Perez-Messina, Ignacio and Graells-Garrido, Eduardo and Lobo, Maria
   Jesus and Hurter, Christophe},
Title = {Modalflow: Cross-Origin Flow Data Visualization for Urban Mobility},
