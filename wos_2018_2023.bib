@article{ WOS:000878150100007,
Author = {Bunel, Mattia},
Title = {A Review of Spatial Imprecision Modelisation Methods},
Journal = {CYBERGEO-EUROPEAN JOURNAL OF GEOGRAPHY},
Year = {2021},
Abstract = {The objective of this article is to present and define the notion of
   spatial imprecision, a term that covers all situations in which a
   spatial object, whatever its nature, finds its limits difficult to
   identify. We present this notion and related concepts, taking care to
   clarify confusing vocabulary and contradictory definitions. This paper
   also lists the different theories and their implementations that allow
   us to model spatial imprecision. The whole program will be based on a
   spun example, allowing to clarify concepts and models.},
