Editor = {Cardaci, A and Fassi, F and Remondino, F},
Title = {INTERACTIVE IMAGE GEOLOCALIZATION IN AN IMMERSIVE WEB APPLICATION},
Booktitle = {8TH INTERNATIONAL WORKSHOP 3D-ARCH: 3D VIRTUAL RECONSTRUCTION AND
   VISUALIZATION OF COMPLEX ARCHITECTURES},
